<!DOCTYPE html>
<html lang="es">
    <!-- Head -->
    <% title = "Ingresar" %>
    <% page = "register" %>
    <%- include("./partials/head") -%>
    <!-- /Head -->
    <body>
        <!-- Header -->
        <%- include("./partials/header") -%>
        <!-- /Header -->

        <main class="page">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <h2 class="title">Bienvenido/a</h2>
                    </div>
                    <div class="col-12 col-md-8 mx-auto my-5">
                        <form action="/usuario/ingresar" method="post">
                            <div class="box p-4">
                                <div class="mb-3 item">
                                    <label class="col-12" for="email">Correo</label>
                                    <input class="col-12" type="email" id="email" name="email">
                                </div>
                                <div class="mb-3 item">
                                    <label class="col-12" for="password">Contrase√±a</label>
                                    <input class="col-12" type="password" id="password" name="password">
                                </div>
                                <div class="item">
                                    <button type="submit">Ingresar</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </main>

        <!-- Footer -->
        <%- include("./partials/footer") -%>
        <!-- /Footer -->
    </body>
</html>